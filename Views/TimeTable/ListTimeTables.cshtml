@model IEnumerable<stonecutter.Models.TimeTable>
@{

}
<!DOCTYPE html>
<html>
    <body>
        <h2>List of Scheduled Installations</h2>
        <table id="timetables">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Name</th>
                    <th>Address</th>
                    <th>Phone number</th>
                    <th>Email</th>
                    <th>Description</th>
                </tr>
            </thead>
            <tbody>
            @foreach (stonecutter.Models.TimeTable t in Model)
            {
                <tr>
                    <td>@t.Id</td>
                    <td>@t.ClientName</td>
                    <td>@t.Address</td>
                    <td>@t.PhoneNumber</td>
                    <td>@t.Email</td>
                    <td>@t.Description</td>
                </tr>
            }
            <tr>
                <td> </td>
                <td> </td>
                <td> </td>
                <td> </td>
                <td> </td>
                <td> </td>
            </tr>
            </tbody>
        </table>

        <a href="~/TimeTable/CreateTimeTable">Schedule an installation</a>
    </body>
</html>
